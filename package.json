{
  "homepage": "http://molnar-atesz.github.io/burrito-translate",
  "name": "burrito-translate",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/molnar-atesz/burrito-translate.git"
  },
  "license": "MIT",
  "config": {
    "app_to_debug": "word",
    "app_type_to_debug": "desktop",
    "dev_server_port": 3000
  },
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite --port 3000",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "office-addin-lint prettier",
    "preview": "vite preview",
    "start": "office-addin-debugging start src/manifest.xml",
    "start:desktop": "office-addin-debugging start src/manifest.xml desktop",
    "start:desktop:preview": "office-addin-debugging start dist/manifest.xml desktop",
    "start:web": "office-addin-debugging start src/manifest.xml web",
    "stop": "office-addin-debugging stop src/manifest.xml",
    "stop:preview": "office-addin-debugging stop dist/manifest.xml",
    "test": "vitest",
    "validate": "office-addin-manifest validate src/manifest.xml"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.47.5",
    "@fluentui/react-icons": "^2.0.235",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.1",
    "@types/office-js": "^1.0.380",
    "@types/office-runtime": "^1.0.35",
    "@types/react": "^18.2.76",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-plugin-office-addins": "^2.1.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.5",
    "jsdom": "^24.0.0",
    "office-addin-cli": "^1.5.9",
    "office-addin-debugging": "^5.0.17",
    "office-addin-dev-certs": "^1.12.2",
    "office-addin-lint": "^2.2.9",
    "office-addin-manifest": "^1.12.11",
    "office-addin-mock": "^2.3.18",
    "office-addin-prettier-config": "^1.2.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.8",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-office-addin": "^2.1.0",
    "vitest": "^1.5.0"
  },
  "prettier": "office-addin-prettier-config"
}
